root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true

[{*.json,yang.settings}]
indent_style = space
indent_size = 2

[*.md]
indent_style = space
indent_size = 2

[*.gradle]
indent_style = tab
indent_size = 4

[gradlew]
indent_style = space
indent_size = 4

[gradlew.bat]
end_of_line = crlf

[*.yml]
indent_style = space
indent_size = 2
